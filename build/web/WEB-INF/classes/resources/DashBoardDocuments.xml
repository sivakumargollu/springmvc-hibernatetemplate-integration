<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="learning.springmvc.controllers.DashBoardDocuments" table="dashboard_documents" catalog="test">
        <id name="id">
            <column name="id"></column>
            <generator class="assigned"></generator>
        </id>
        <property name="userID" column="user_id" ></property>
        <property name="cmsUrl" column="cms_url" ></property>
        <property name="dateTime" column="datetime_time" ></property>
    </class>
</hibernate-mapping>